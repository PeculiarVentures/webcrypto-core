{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "types/**/*.d.ts"
        ]
    },
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "max-line-length": false,
        "member-ordering": false,
        "object-literal-sort-keys": false,
        "forin": false,
        "no-empty-interface": false,
        "no-bitwise": false
    },
    "rulesDirectory": []
}